{
	"name": "comanda-server",
	"version": "0.1.0",
	"description": "Comand Realtime App - Server Side",
	"type": "module",
	"scripts": {
		"start": "node dist/server.js",
		"build": "tsc -p tsconfig.json",
		"dev": "node --watch --no-warnings --enable-source-maps --import tsx src/server.ts",
		"watch": "nodemon --watch src/ -e ts --exec \"npm run dev\""
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/node": "^20.14.2",
		"nodemon": "^3.1.3",
		"tsx": "^4.15.3",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"socket.io": "^4.7.5",
		"shared": "file:../shared"
	}
}
